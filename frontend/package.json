{
  "name": "sample.ui5.uaa.ui",
  "version": "1.2.1",
  "engines": {
    "node": "^18"
  },
  "dependencies": {
    "@sap/approuter": "^14.3.2"
  },
  "scripts": {
    "start": "node node_modules/@sap/approuter/approuter.js",
    "serve": "ui5 serve -o",
