{
  "_version": "1.21.0",
  "sap.app": {
    "_version": "1.3.0",
    "id": "sample.ui5.uaa",
    "type": "application",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "i18n": {
      "bundleUrl": "i18n/i18n.properties",
      "supportedLocales": [""],
      "fallbackLocale": ""
    },
    "dataSources": {
      "mainService": {
        "uri": "/ci/",
        "type": "OData",
        "settings": {
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "_version": "1.3.0",
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "_version": "1.3.0",
    "rootView": {
      "viewName": "sample.ui5.uaa.view.App",
      "type": "XML",
      "async": true
    },
    "dependencies": {
      "minUI5Version": "1.96.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.f": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "app": {
        "type": "sap.ui.model.json.JSONModel"
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleUrl": "i18n/i18n.properties",
          "supportedLocales": [""],
          "fallbackLocale": "",
          "async": true
        }
      },
      "": {
        "dataSource": "mainService",
        "settings": {
          "synchronizationMode": "None",
          "earlyRequests": true,
          "operationMode": "Server",
          "groupId": "$direct",
          "autoExpandSelect": true
        },
        "preload": true
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/styles.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "sample.ui5.uaa.view",
        "controlId": "rootContainer",
        "controlAggregation": "pages",
        "bypassed": {
          "target": ["notFound"]
        },
        "async": true
      },
      "targets": {
        "notFound": {
          "viewName": "NotFound",
          "transition": "show"
        },
        "home": {
          "controlAggregation": "pages",
          "viewName": "Home",
          "viewId": "Home",
          "viewLevel": 1,
          "transition": "show"
        }
      },
      "routes": [
        {
          "pattern": "",
          "name": "default",
          "target": ["home"]
        }
      ]
    }
  }
}
